CREATE TABLE GENRE (ID SERIAL PRIMARY KEY, NAME CHAR(255) NOT NULL );
CREATE TABLE PERSON (ID SERIAL PRIMARY KEY, FIRST_NAME CHAR(255) NOT NULL, LAST_NAME CHAR(255) NOT NULL, BITRH DATE, EVALUATION FLOAT, HEIGHT INT );
CREATE TABLE MOVIE (ID SERIAL PRIMARY KEY, FILMWEB_ID INT NOT NULL, TITLE CHAR(255) NOT NULL, ORIGINAL_TITLE CHAR(255) NOT NULL, PREMIERE DATE NOT NULL, DURATION INT NOT NULL, EVALUATION FLOAT, PRODUCTION CHAR(255) NOT NULL, IS_ADAPTATION BOOLEAN, BOOK_TITLE CHAR(255) );
CREATE TABLE AWARD (ID SERIAL PRIMARY KEY, NAME CHAR(255) NOT NULL );
CREATE TABLE MOVIE_GENRE (ID SERIAL PRIMARY KEY, MOVIE_ID INT NOT NULL, GENRE_ID INT NOT NULL );
CREATE TABLE MOVIE_PERSON (ID SERIAL PRIMARY KEY, MOVIE_ID INT NOT NULL, PERSON_ID INT NOT NULL, ROLE CHAR(255) NOT NULL );
CREATE TABLE MOVIE_AWARD (ID SERIAL PRIMARY KEY, MOVIE_ID INT NOT NULL, AWARD_ID INT NOT NULL, YEAR INT NOT NULL, TYPE CHAR(255) NOT NULL, CATEGORY TEXT, AWARD_NAME CHAR(255) );
CREATE TABLE AWARD_MOVIE_PERSON (ID SERIAL PRIMARY KEY, MOVIE_PERSON INT NOT NULL, AWARD_ID INT NOT NULL, YEAR INT NOT NULL, TYPE CHAR(255) NOT NULL, CATEGORY TEXT, AWARD_NAME CHAR(255) );
