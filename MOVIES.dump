CREATE TABLE GENRES (ID INT PRIMARY KEY NOT NULL, NAME CHAR(255) NOT NULL );
CREATE TABLE PEOPLE (ID INT PRIMARY KEY NOT NULL, FIRST_NAME CHAR(255) NOT NULL, LAST_NAME CHAR(255) NOT NULL, BITRH DATE, EVALUATION FLOAT, HEIGHT INT );
CREATE TABLE MOVIES (ID INT PRIMARY KEY NOT NULL, TITLE CHAR(255) NOT NULL, ORIGINAL_TITLE CHAR(255) NOT NULL, PREMIERE DATE NOT NULL, DURATION INT NOT NULL, EVALUATION FLOAT, PRODUCTION CHAR(255) NOT NULL, IS_ADAPTATION BOOLEAN, BOOK_TITLE CHAR(255) );
CREATE TABLE AWARDS (ID INT PRIMARY KEY NOT NULL, NAME CHAR(255) NOT NULL );
CREATE TABLE MOVIES_GENRES (ID INT PRIMARY KEY NOT NULL, MOVIE_ID INT NOT NULL, GENRE_ID INT NOT NULL );
CREATE TABLE MOVIES_PEOPLE (ID INT PRIMARY KEY NOT NULL, MOVIE_ID INT NOT NULL, HUMAN_ID INT NOT NULL, ROLE CHAR(255) NOT NULL );
CREATE TABLE MOVIES_AWARDS (ID INT PRIMARY KEY NOT NULL, MOVIE_ID INT NOT NULL, AWARD_ID INT NOT NULL, YEAR INT NOT NULL, TYPE CHAR(255) NOT NULL, CATEGORY TEXT, AWARD_NAME CHAR(255) );
CREATE TABLE AWARDS_MOVIES_PEOPLE (ID INT PRIMARY KEY NOT NULL, MOVIES_PEOPLE INT NOT NULL, AWARD_ID INT NOT NULL, YEAR INT NOT NULL, TYPE CHAR(255) NOT NULL, CATEGORY TEXT, AWARD_NAME CHAR(255) );
